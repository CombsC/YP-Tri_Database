@model IEnumerable<YPTriMembership.Models.Event>

@{
    ViewBag.Title = "Index";
}

<h2>Current Events!</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Street_Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.State_Abr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Zip_Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Street_Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State_Abr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Zip_Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
    
        <td>      
            @Html.ActionLink("Details", "DetailsEvent", new { id=item.Event_id })        
        </td>
    </tr>
}

</table>
